{
  "name": "micro-utils",
  "version": "0.2.0",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "types": "./dist/types/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./publicPath": {
      "import": "./dist/es/public-path.js",
      "default": "./dist/es/public-path.js"
    },
    "./micro-app": {
      "import": "./dist/es/micro-app/index.js",
      "types": "./dist/types/micro-app/index.d.ts",
      "default": "./dist/lib/micro-app/index.js"
    },
    "./config": {
      "import": "./dist/es/scripts/config.js",
      "types": "./dist/types/scripts/config.d.ts",
      "default": "./dist/lib/scripts/config.js"
    }
  },
  "scripts": {
    "dev": "modern dev",
    "build": "modern build",
    "build:watch": "modern build -w",
    "reset": "rimraf ./**/node_modules",
    "lint": "modern lint",
    "change": "modern change",
    "bump": "modern bump",
    "pre": "modern pre",
    "change-status": "modern change-status",
    "gen-release-note": "modern gen-release-note",
    "release": "modern release",
    "new": "modern new",
    "upgrade": "modern upgrade"
  },
  "bin": {
    "custombuild": "./build.mjs"
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "@micro-zoe/micro-app": "^1.0.0-rc.6",
    "@module-federation/runtime": "^0.3.5",
    "@rspack/core": "^1.0.0",
    "semver": "^7.6.3",
    "@modern-js/eslint-config": "2.56.2",
    "@modern-js/module-tools": "2.56.2",
    "@rsbuild/core": "^1.0.1-beta.6",
    "@rsbuild/plugin-react": "^1.0.1-beta.6",
    "@rsbuild/plugin-sass": "^1.0.1-beta.6"
  },
  "devDependencies": {
    "@types/node": "~16.11.7",
    "@types/semver": "^7.5.8",
    "prettier": "~2.8.1",
    "rimraf": "~3.0.2",
    "typescript": "~5.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "sideEffects": [],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
